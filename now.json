{
  "version": 2,
  "builds": [
    {
      "src": "*.ts",
      "use": "@now/node"
    },
    {
      "src": "src/*.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/callback/*",
      "dest": "server.ts"
    },
    {
      "src": "/graphql/*",
      "dest": "/src/gqlServer.ts"
    }
  ]
}
